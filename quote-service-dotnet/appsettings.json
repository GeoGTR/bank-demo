{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://discovery-server.dev.svc.cluster.local/eureka",
      "ValidateCertificates":  false,
      "ShouldFetchRegistry": false
    },
  "Management": {
    "Tracing": {
      "AlwaysSample": true
    },
    "endpoints": {
      "actuator": {
        "exposure": {
          "include": [ "*" ]
        }
      }
    },
    "metrics": {
      "observer": {
        "EventCounterEvents": "true",
        "GCEvents": "true",
        "ThreadPoolEvents": "true"
      },
      "export": {
        "wavefront": {
          "Uri": "https://longboard.wavefront.com",
          "apiToken": "asd",
          "step": "8080"
        }
      }
    }
  },
  "wavefront": {
    "application": {
      "name": "quote-service-dotnet",
      "service": "banking-demo"
    }
  },
  "instance": {
    "port": 8080,
    "hostName": "quote-service",
    "app": "quote-service"
  }
}
}
