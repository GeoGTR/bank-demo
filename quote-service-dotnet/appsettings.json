{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Steeltoe.Management.OpenTelemetry.Exporters": "Trace"
    }
  },
  "AllowedHosts": "*",
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://discovery-server.dev.svc.cluster.local/eureka",
      "ValidateCertificates":  false,
      "ShouldFetchRegistry": false
    }
  },
  "spring": {
      "application": {
        "name": "quote-service"
      }
  },
  "Management": {
    "Tracing": {
      "AlwaysSample": true,
      "useShortTraceIds ": true,
      "exporter": {
        "zipkin": {
          "serviceName": "quote-service",
          "endpoint": "http://wf-proxy:9411/api/v2/spans",
          "validateCertificates": false,
          "useShortTraceIds ": true
        }
      }
    },
    "endpoints": {
      "actuator": {
        "exposure": {
          "include": [ "*" ]
        }
      },
      "Health": {
        "ShowDetails": "Always"
      },
      "Prometheus": {
        "Enabled": true
      }
    },
    "metrics": {
      "observer": {
        "EventCounterEvents": "true",
        "GCEvents": "true",
        "ThreadPoolEvents": "true",
        "AspNetCoreHosting": true
      },
      "export": {
        "wavefront": {
          "Uri": "proxy://wf-proxy:2878",
          "step": "5000"
        }
      }
    }
  },
  "wavefront": {
    "application": {
      "name": "quote-service-dotnet",
      "service": "banking-demo"
    }
  },
  "instance": {
    "port": 8080,
    "hostName": "quote-service",
    "app": "quote-service"
  }
}
